{
  "name": "cache-locking",
  "version": "1.0.0",
  "description": "Lease-based single-flight cache locking with pluggable adapters.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "examples",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config vitest-config.ts",
    "test:e2e": "INTEGRATION_TESTS=1 vitest run --config vitest-e2e.config.ts",
    "test:integration": "INTEGRATION_TESTS=1 vitest run --config vitest-config.ts",
    "test:watch": "vitest --config vitest-config.ts",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "effect": "^3.7.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@cloudflare/workers-types": "^4.20250124.0",
    "memjs": "^1.3.0",
    "mongodb": "^6.0.0",
    "pg": "^8.0.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@cloudflare/workers-types": "^4.20250124.0",
    "@types/memjs": "^1.3.3",
    "@types/node": "^22.13.4",
    "@types/pg": "^8.16.0",
    "memjs": "^1.3.0",
    "miniflare": "^3.0.0",
    "mongodb": "^6.0.0",
    "pg": "^8.0.0",
    "prettier": "^3.8.1",
    "redis": "^4.6.0",
    "testcontainers": "^10.21.0",
    "tsup": "^8.3.0",
    "typescript": "^5.5.3",
    "vitest": "^2.1.5"
  },
  "private": true
}
